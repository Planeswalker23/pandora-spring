<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!-- 不声明 id/name 属性 -->
    <bean class="io.walkers.planes.pandora.spring.ioc.User">
        <property name="id" value="2"/>
        <property name="name" value="PlanesWalker23-lookup-byType"/>
    </bean>

    <!-- 带注解的 Bean -->
    <bean id="userWithAnnotation" class="io.walkers.planes.pandora.spring.ioc.UserWithAnnotation">
        <property name="id" value="4"/>
        <property name="name" value="PlanesWalker23-lookup-byAnnotation"/>
    </bean>
</beans>